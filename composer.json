{
    "name": "javaabu/settings",
    "description": "File based settings system for Laravel",
    "homepage": "https://github.com/javaabu/settings",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "helpers",
        "settings"
    ],
    "authors": [
        {
            "name": "Athfan Khaleel",
            "email": "athfan@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "anlutro/l4-settings": "^1.4",
        "spatie/laravel-activitylog": "^4.8"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
